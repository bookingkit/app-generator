{
    "chmod": "0755",
    "directories": [
        "app",
        "bootstrap",
        "config",
        "vendor",
        "app/Stubs"
    ],
    "files": [
        "composer.json"
    ],
    "finder": [
        {
            "name": "*.stub",
            "in": "app/Stubs"
        }
    ],
    "exclude-composer-files": false,
    "compression": "GZ",
    "compactors": [
        "KevinGH\\Box\\Compactor\\Php",
        "KevinGH\\Box\\Compactor\\Json"
    ],
    "main": "app-generator"
}
